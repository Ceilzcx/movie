<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.movie.ui.movie.MovieFragment"
        android:label="deeplearning"
        tools:layout="@layout/fragment_movie" />

    <fragment
        android:id="@+id/navigation_prediction"
        android:name="com.example.movie.ui.prediction.PredictionFragment"
        android:label="deeplearning"
        tools:layout="@layout/fragment_prediction" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.movie.ui.user.UserFragment"
        android:label="deeplearning"
        tools:layout="@layout/fragment_user" />
</navigation>